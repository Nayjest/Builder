    "license": "MIT",
    "authors": [
        {
            "name": "Vitalii Stepanenko",
            "email": "mail@vitaliy.in"
        }
    ],
    "require": {
        "nayjest/manipulator": "~1",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "dev-master@dev",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
